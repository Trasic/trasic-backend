{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "import/no-unresolved": 0,
        "linebreak-style": ["error", "windows"]
    },
    "settings": {
        "import/resolver": {
            "node": {
                "extensions": ".js"
            }
        }
    }
}
